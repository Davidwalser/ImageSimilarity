@incollection{GAVALI201999,
title = {Chapter 6 - Deep Convolutional Neural Network for Image Classification on CUDA Platform},
editor = {Arun Kumar Sangaiah},
booktitle = {Deep Learning and Parallel Computing Environment for Bioengineering Systems},
publisher = {Academic Press},
pages = {99-122},
year = {2019},
isbn = {978-0-12-816718-2},
doi = {https://doi.org/10.1016/B978-0-12-816718-2.00013-0},
url = {https://www.sciencedirect.com/science/article/pii/B9780128167182000130},
author = {Pralhad Gavali and J. Saira Banu},
keywords = {Deep learning, Convolutional neural networks, TensorFlow, CUDA, CIFAR},
abstract = {One of the main problems in computer vision is the image classification problem, which is concerned with determining the presence of visual structures in an input image. Image classification analyzes the numerical properties of various image features and organizes data into categories. In recent years, many advanced classification approaches, such as artificial neural networks, fuzzy-sets, and expert systems, have been widely applied for image classification, but each of them having some problems and their accuracy level is comparatively less. One of the advanced classification approaches is to use the deep convolutional neural network (CNN) architecture of deep learning which obtains successful results in solving many machine learning problems. In this study, the image classification process is performed by using like an LeNet network model. The Keras library, which is regularly utilized for deep learning is used. The proposed method is trained and tested with images of cats and dogs taken from the CIFAR dataset. We use 1000 labeled data for training and 500 unlabeled data for testing. For training we pick the first 1000 images, and for validation we pick only 100 images. Deep convolutional neural networks are used for classification, and GPU technology is used for parallel processing. In this method, the GPU technology and classification are evaluated with acceptable accuracy rate and speed performance. The experimental study is done with the Ubuntu 16.04 operating system using an NVIDIA Geforce GTX 680 with 2 GB of memory and model performance is measured by classification accuracy with 95\% or above.}
}
